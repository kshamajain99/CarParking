<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kshamajain.carparkingapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Car Parking Application"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddUserActivity"
            android:label="@string/title_activity_add_user" >
        </activity>
        <activity
            android:name=".SearchUserActivity"
            android:label="@string/title_activity_search_user" >
        </activity>
        <activity
            android:name=".AvailabilityActivity"
            android:label="@string/title_activity_availability" >
        </activity>
    </application>

</manifest>
